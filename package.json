{
  "name": "@octokit/openapi-types",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0-development",
  "description": "Generated TypeScript definitions based on GitHub's OpenAPI spec for https://api.github.com",
  "repository": "github:octokit/openapi-types.ts",
  "author": "Gregor Martynus (https://twitter.com/gr2m)",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "download": "node scripts/download",
    "generate-types": "node scripts/generate-types",
    "update-package": "node scripts/update-package",
    "lint": "prettier --check '*.{md,json}' 'scripts/*.js' 'packages/**/*.{ts,md,json,d.ts}'",
    "lint:fix": "prettier --write '*.{md,json}' 'scripts/*.js' 'packages/**/*.{ts,md,json,d.ts}'"
  },
  "devDependencies": {
    "@octokit/core": "^4.0.0",
    "@octokit/plugin-paginate-rest": "^3.1.0",
    "github-enterprise-server-versions": "^1.0.0",
    "openapi-typescript": "^5.0.0",
    "prettier": "2.7.1"
  },
  "prettier": {},
  "release": {
    "branches": [
      "main",
      "beta"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "packages/openapi-types"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "packages/openapi-types-ghes-3.2"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "packages/openapi-types-ghes-3.3"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "packages/openapi-types-ghes-3.4"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "packages/openapi-types-ghes-3.5"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "packages/openapi-types-ghes-3.6"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "packages/openapi-types-github.ae"
        }
      ]
    ]
  },
  "renovate": {
    "extends": [
      "github>octokit/.github"
    ]
  }
}
